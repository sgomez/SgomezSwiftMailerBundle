<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service class="Sgomez\DebugSwiftMailerBundle\DataCollector\SwiftMailerDataCollector" id="sgomez_swiftmailer.data_collector">
            <argument type="service" id="service_container" />
            <tag name="data_collector" template="@SgomezDebugSwiftMailer/data_collector/sgomez_swiftmailer.html.twig" id="sgomez_swiftmailer" />
        </service>
    </services>
</container>
